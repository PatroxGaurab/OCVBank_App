CREATE DATABASE OCV;

CREATE USER 'public_user'@'localhost' IDENTIFIED BY 'thisUSERdoesITaLL';
GRANT SELECT, INSERT, UPDATE ON `OCV`.* TO 'public_user'@'localhost';
USE OCV;
CREATE TABLE REGISTRATION 
(
 FULL_NAME VARCHAR(50) NOT NULL,
 REGISTRATION_NO VARCHAR(9) PRIMARY KEY,
 EMAIL_ID VARCHAR(50) NOT NULL,
 PASS_WORD VARCHAR(50) NOT NULL,
 CHECK_FIELD VARCHAR(5) NOT NULL
);

